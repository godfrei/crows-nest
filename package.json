    "moment": "^2.24.0",
    "node-sass": "^5.0.0",
    "prismjs": "^1.17.1",
    "react": "^17.0.1",
    "react-body-classname": "^1.3.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
